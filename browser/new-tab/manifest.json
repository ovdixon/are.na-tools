{
    "manifest_version": 3,
    "name": "Are.na New Tab",
    "version": "1.0",
    "icons": {
        "128": "img/logo.png"
    },
    "host_permissions": [
        "https://dev.are.na/*",
        "https://api.are.na/v2/*",
        "https://arena-mv3-auth.ovdixon.workers.dev/*"
    ],
    "permissions": [
        "storage",
        "identity"
    ],
    "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}